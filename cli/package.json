{
  "name": "create-course-app",
  "version": "7.24.2",
  "description": "Create web application with the Course Builder stack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joelhooks/create-course-app.git",
    "directory": "cli"
  },
  "keywords": [
    "create-course-app",
    "create-t3-app",
    "init.tips",
    "next.js",
    "t3-stack",
    "tailwind",
    "tRPC",
    "typescript"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "create-course-app": "./dist/index.js"
  },
  "files": [
    "dist",
    "template",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    ".yarnrc.yml",
    "package.json"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .turbo node_modules",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern --write",
    "format:check": "prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern --check",
    "release": "changeset version",
    "pub:beta": "pnpm build && npm publish --tag beta",
    "pub:next": "pnpm build && npm publish --tag next",
    "pub:release": "pnpm build && npm publish"
  },
  "dependencies": {
    "@clack/core": "^0.3.2",
    "@clack/prompts": "^0.6.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "chalk": "5.2.0",
    "commander": "^10.0.0",
    "execa": "^7.1.1",
    "fs-extra": "^11.1.1",
    "gradient-string": "^2.0.2",
    "ora": "6.3.1",
    "sort-package-json": "^2.4.1"
  },
  "devDependencies": {
    "@auth/drizzle-adapter": "^0.3.6",
    "@coursebuilder/tsconfig": "^0.0.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@planetscale/database": "1.14.0",
    "@prisma/client": "^5.6.0",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "10.45.0",
    "@trpc/next": "10.45.0",
    "@trpc/react-query": "10.45.0",
    "@trpc/server": "10.45.0",
    "@types/fs-extra": "^11.0.1",
    "@types/gradient-string": "^1.1.2",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.17.0",
    "drizzle-kit": "^0.19.13",
    "drizzle-orm": "^0.28.5",
    "next": "14.1.0",
    "next-auth": "^4.24.5",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^2.2.1",
    "tailwindcss": "^3.3.3",
    "tsup": "6.7.0",
    "type-fest": "^3.7.0",
    "typescript": "^5.2.2",
    "zod": "^3.22.4"
  }
}
