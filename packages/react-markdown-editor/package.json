{
	"name": "@coursebuilder/react-markdown-editor",
	"version": "6.1.2",
	"description": "FORK OF @uiw/react-markdown-editor, A markdown editor with preview, implemented with React.js and TypeScript.",
	"homepage": "https://uiwjs.github.io/react-markdown-editor",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./dist/index.css": "./dist/index.css"
	},
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/uiwjs/react-markdown-editor"
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build": "NODE_OPTIONS='--max-old-space-size=16384' tsup",
		"dev": "NODE_OPTIONS='--max-old-space-size=16384' tsup --watch",
		"test": "vitest run -c ../utils/vitest.config.ts"
	},
	"author": "Kenny Wong <wowohoo@qq.com>",
	"license": "MIT",
	"files": [
		"dist",
		"src"
	],
	"style": "dist/index.css",
	"peerDependencies": {
		"@babel/runtime": ">=7.10.0",
		"react": ">=19.0.0",
		"react-dom": ">=19.0.0"
	},
	"keywords": [
		"array-viewer",
		"component",
		"interactive",
		"interactive-json",
		"json",
		"json-component",
		"json-display",
		"json-tree",
		"json-view",
		"json-viewer",
		"json-inspector",
		"json-tree",
		"react",
		"react-component",
		"react-json",
		"theme",
		"tree",
		"tree-view",
		"treeview"
	],
	"dependencies": {
		"@babel/runtime": "^7.22.6",
		"@codemirror/commands": "^6.6.0",
		"@codemirror/lang-markdown": "^6.3.1",
		"@codemirror/language-data": "^6.5.1",
		"@codemirror/search": "^6.5.8",
		"@codemirror/state": "^6.5.0",
		"@codemirror/view": "^6.35.3",
		"@lezer/highlight": "^1.2.1",
		"@uiw/codemirror-extensions-events": "^4.23.7",
		"@uiw/codemirror-themes": "^4.23.7",
		"@uiw/react-codemirror": "^4.23.7",
		"@uiw/react-markdown-preview": "^5.1.3",
		"y-codemirror.jh": "0.3.5-joel-fork",
		"y-partykit": "^0.0.32",
		"yjs": "^13.6.10"
	},
	"devDependencies": {
		"@testing-library/react": "^14.0.0",
		"@types/react": "npm:types-react@19.0.0-rc.1",
		"@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
		"@types/react-test-renderer": "19.0.0",
		"esbuild-plugin-less": "^1.3.14",
		"less": "^4.2.1",
		"react": "19.0.0-rc-02c0e824-20241028",
		"react-dom": "19.0.0-rc-02c0e824-20241028",
		"react-test-renderer": "^19.0.0",
		"tsup": "8.0.2"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "npm:types-react@19.0.0-rc.1",
			"@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
		}
	}
}
