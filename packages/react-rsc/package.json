{
	"name": "@coursebuilder/react-rsc",
	"version": "0.0.3",
	"description": "Course Builder Package",
	"scripts": {
		"clean": "rm -rf ./dist",
		"build": "tsup",
		"dev": "tsup --watch"
	},
	"keywords": [
		"coursebuilder"
	],
	"author": "",
	"license": "ISC",
	"type": "module",
	"files": [
		"dist",
		"src"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"import": "./dist/client.js",
			"require": "./dist/client.cjs"
		}
	},
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@types/react": "^18.3.1",
		"@types/react-dom": "^18.3.0",
		"esbuild-react18-useclient": "^1.0.7",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"tsup": "8.0.2"
	},
	"peerDependencies": {
		"react": "^18",
		"react-dom": "^18"
	},
	"dependencies": {
		"@coursebuilder/core": "^0.2.0",
		"@coursebuilder/ui": "^1.0.11",
		"@hookform/resolvers": "^3.3.2",
		"react-hook-form": "^7.48.0",
		"zod": "^3.22.4"
	}
}
