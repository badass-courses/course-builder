{
	"name": "@coursebuilder/next",
	"version": "0.0.29",
	"description": "Next for Course Builder.",
	"scripts": {
		"clean": "rm -rf *.js *.d.ts* lib providers query",
		"build": "pnpm clean && tsc",
		"dev": "tsc --watch",
		"test": "vitest run -c ../utils/vitest.config.ts"
	},
	"keywords": [
		"coursebuilder",
		"drizzle"
	],
	"author": "",
	"license": "ISC",
	"type": "module",
	"types": "./index.d.ts",
	"files": [
		"*.d.ts*",
		"*.js",
		"lib",
		"providers",
		"query",
		"server",
		"src"
	],
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./index.js"
		},
		"./server": {
			"types": "./server/index.d.ts",
			"import": "./server/index.js"
		},
		"./providers": {
			"types": "./providers/index.d.ts",
			"import": "./providers/index.js"
		},
		"./query": {
			"types": "./query/index.d.ts",
			"import": "./query/index.js"
		}
	},
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@axiomhq/js": "^1.0.0",
		"@coursebuilder/core": "1.1.0",
		"@coursebuilder/utils-browser": "workspace:*",
		"@sindresorhus/slugify": "^2.2.1",
		"@upstash/redis": "^1.34.4",
		"inngest": "3.35.0",
		"next-themes": "^0.3",
		"zod": "3.24.2"
	},
	"devDependencies": {
		"@amplitude/analytics-browser": "^2.6.1",
		"@auth/core": "0.37.2",
		"@mux/mux-player-react": "^3.8.0",
		"@types/react": "19.2.7",
		"drizzle-orm": "0.36.0",
		"next": "16.0.7",
		"next-auth": "5.0.0-beta.25",
		"react": "19.2.3"
	},
	"peerDependencies": {
		"@amplitude/analytics-browser": "^2.6.1",
		"@mux/mux-player-react": "^3.8.0",
		"drizzle-orm": "0.36.0",
		"next": "16.0.7",
		"next-auth": "5.0.0-beta.25",
		"next-themes": "^0.3",
		"react": "19.2.3"
	},
	"peerDependenciesMeta": {
		"@amplitude/analytics-browser": {
			"optional": true
		},
		"@mux/mux-player-react": {
			"optional": true
		},
		"drizzle-orm": {
			"optional": true
		}
	}
}
