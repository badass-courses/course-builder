{
	"name": "@coursebuilder/next",
	"version": "0.0.20",
	"description": "Next for Course Builder.",
	"scripts": {
		"clean": "rm -rf *.js *.d.ts* lib providers",
		"build": "pnpm clean && tsc",
		"dev": "tsc --watch",
		"test": "vitest run -c ../utils/vitest.config.ts"
	},
	"keywords": [
		"coursebuilder",
		"drizzle"
	],
	"author": "",
	"license": "ISC",
	"type": "module",
	"types": "./index.d.ts",
	"files": [
		"*.d.ts*",
		"*.js",
		"lib",
		"providers",
		"src"
	],
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./index.js"
		}
	},
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@coursebuilder/core": "0.2.4",
		"inngest": "^3.22.5"
	},
	"devDependencies": {
		"@auth/core": "^0.37.2",
		"next": "15.0.2",
		"next-auth": "5.0.0-beta.25",
		"react": "19.0.0-rc-02c0e824-20241028"
	},
	"peerDependencies": {
		"next": "15.0.2",
		"next-auth": "5.0.0-beta.25",
		"react": "19.0.0-rc-02c0e824-20241028"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "npm:types-react@19.0.0-rc.1",
			"@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
		}
	}
}
