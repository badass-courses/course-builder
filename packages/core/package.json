{
  "name": "@coursebuilder/core",
  "version": "0.0.1",
  "description": "Core package for Course Builder",
  "scripts": {
    "clean": "rm -rf *.js *.d.ts* lib",
    "test": "pnpm test:mysql && pnpm test:sqlite && pnpm test:pg",
    "test:mysql": "pnpm clean && ./test/mysql/test.sh",
    "test:sqlite": "pnpm clean && ./test/sqlite/test.sh",
    "test:pg": "pnpm clean && ./test/pg/test.sh",
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "keywords": [
    "coursebuilder",
    "drizzle"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "types": "./index.d.ts",
  "files": [
    "*.d.ts*",
    "*.js",
    "lib",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./adapters": {
      "types": "./adapters.d.ts"
    },
    "./types": {
      "types": "./types.d.ts"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@auth/core": "^0.28.0"
  },
  "devDependencies": {
    "tsup": "6.7.0"
  }
}
