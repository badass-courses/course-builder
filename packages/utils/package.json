			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"unplugin-swc": "^1.4.4"
	},
	"devDependencies": {
		"@coursebuilder/core": "workspace:*",
		"@preact/preset-vite": "^2.8.1",
		"dotenv": "^10.0.0",
		"tsup": "6.7.0"
	}
