{
	"name": "@coursebuilder/utils",
	"version": "0.0.0",
	"description": "Course Builder Package",
	"scripts": {
		"clean": "rm -rf ./dist",
		"build": "tsup",
		"dev": "tsup --watch"
	},
	"keywords": [
		"coursebuilder"
	],
	"author": "",
	"license": "ISC",
	"type": "module",
	"types": "./dist/index.d.ts",
