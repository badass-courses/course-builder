{
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": ".",
		"declaration": true,
		"declarationMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": true,
		"module": "ESNext",
		"moduleResolution": "node",
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"strict": true,
		"strictNullChecks": true,
		"stripInternal": true,
		"target": "es2020"
	}
}
